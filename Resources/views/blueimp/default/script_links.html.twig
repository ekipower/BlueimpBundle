{#
	Expected parameters:
	- options
	- blueimpOptions
#}
<script>
document.getElementById('{{ options.linksId }}').onclick = function (event) {
    event = event || window.event;
    var target = event.target || event.srcElement,
        link = target.src ? target.parentNode : target,
        options =  {
			{% for optionKey, optionValue in blueimpOptions %}
				{% if optionValue is same as(true) %}
					'{{ optionKey }}': true,
				{% elseif optionValue is same as(false) %}
					'{{ optionKey }}': false,
				{% else %}
					'{{ optionKey }}': '{{ optionValue }}',
				{% endif %}
			{% endfor %}
		},
        links = this.getElementsByTagName('a');
    blueimp.Gallery(links, options);
};
</script>

